## hidden code

def mk_coverage():
    covered = set()
    target = set(range(6))
    count = 0
    
    def coverage (func):
        nonlocal covered, target, count

        def wrapper (left, right, value=None):
            nonlocal covered, count
            if value is None:
                covered.add(0)
            else:
                covered.add(1)
            if isinstance (left, Leaf0):
                covered.add(2)
            else:
                covered.add (3)
            if isinstance (right, Leaf0):
                covered.add (4)
            else:
                covered.add (5)
            r = func (left, right, value)
            count += 1
            return r

        if func == "achieved": return len(covered)
        if func == "required": return len(target)
        if func == "count" : return count
        functools.update_wrapper (wrapper, func)    
        return wrapper

    return coverage

coverage = mk_coverage ()
try:
    Node = coverage (Node)
except:
    pass